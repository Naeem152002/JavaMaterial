		System.out.println(fi.exists());
		System.out.println(fi.canRead());
		
		File fi2=new File("C:\\Users\\Akash\\Desktop\\JavaNotes\\New folder\\New folder\\Akash");
		
		if(!fi.exists())
		{
			fi.mkdir();
