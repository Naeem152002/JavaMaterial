package com.thread.practice;

interface I{
	void show();
}


class A implements I{
	
	public A(I i) {
		this(i);
	}
	

	@Override
	public void show() {
		
	}
	
}
public class Test1 {
	public static void main(String[]args) {
		
		I i=new I() {

			@Override
			public void show() {
				System.out.println("hello");
				
			}
			
		};
		
	}

}
