package com.sorting;

public class Test14 {
	public static void pairArray(int a[]) {
		for(int i=0;i<a.length;i++) {
			int curr=a[i];
			for(int j=i+1;j<a.length;j++) {
				System.out.print("("+curr+","+a[i]+")");
			}
			System.out.println();
		}
		
	}
public static void main(String[]args) {
	int a[]= {2,3,5,10,34,25};
	pairArray(a);
	for(int i=0;i<a.length;i++) {
		System.out.println(a[i]);
	}
}
}
