// method overrding 
class Bike {
	void run() {
		System.out.println("bike is running ....");
	}
}
public class Splender extends Bike {
void run() {
	super.run();
	System.out.println("splender is running..");
}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
Splender s= new Splender ();
s.run();
	}

}
