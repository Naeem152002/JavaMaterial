package com.arrays;

public class Test10 {
	//using kedan,s algorthim
	//Tc-n
	
	public static void maxSumSubArrays(int a[]) {
		int sum=0;
		int max=Integer.MIN_VALUE;
		for(int i=0;i<a.length;i++) {
			sum=sum+a[i];
			if(sum<0) {
				sum=0;
			}
		}
	}
	
	public static void main(String[]args) {
		int a[]= {1,-2,6,-1,3};
		maxSumSubArrays(a);
	}

}
