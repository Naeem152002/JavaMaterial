package com.arrays;

public class Test4 {
	
	//Tx=n,Tc=1;
	
	public static void reverseArray(int a[]) {
		int start=0; int end=a.length-1;
		
		while(start<end) {
			int temp=a[start];
			a[start]=a[end];
			a[end]=temp;
		}
	}
	
	public static void main(String[]args) {
		int a[]= {2,4,6,8,10,12,14};
		int key=12;
		for(int i=0;i<a.length;i++) {
			System.out.println(a[i]);
		}
		
		
	}

}
